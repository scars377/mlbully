:global
	html
	body
	#wrapper
		position absolute
		width  100%
		height 100%

	#wrapper
		background-color #FCF19D
		overflow: hidden;

.board
	position absolute
	// width  1024px
	// height 768px
	width  100%
	height 100%
	color: #fff
	text-align: center
	font-family 'Impact'
	user-select none


.head
	position: relative
	display: inline-block
	margin: 20px 0
	width: 980px
	height 270px

.team
	position absolute
	cursor pointer
	width 580px
	height 270px
	top 0
	left 0
	background-size: cover

	border 1px solid white
	box-sizing border-box
	color rgba(#fff,.8)
	font-size 200px
	line-height @height


